{
  "name": "term-project-teamp",
  "version": "1.0.0",
  "description": "term-project-teamp created by GitHub Classroom Trello: https://trello.com/b/wiXj0IKg/csc667-867-uno",
  "main": "backend/server.js",
  "scripts": {
    "start": "node backend/server.js",
    "start:dev": "concurrently \"npm:server:dev\" \"npm:build:dev\"",
    "server:dev": "NODE_ENV=development nodemon -e js,pug --watch backend ./server.js",
    "build": "webpack",
    "build:dev": "webpack --watch",
    "prepare": "husky install",
    "db:create": "node-pg-migrate create --",
    "db:migrate": "node-pg-migrate up",
    "db:rollback": "node-pg-migrate down",
    "postinstall": "npm run db:migrate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/csc-667-spring-2023-roberts/term-project-teamp.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/csc-667-spring-2023-roberts/term-project-teamp/issues"
  },
  "homepage": "https://github.com/csc-667-spring-2023-roberts/term-project-teamp#readme",
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "http-errors": "^2.0.0",
    "morgan": "^1.10.0",
    "node-pg-migrate": "^6.2.2",
    "pg": "^8.10.0",
    "pg-promise": "^11.4.3",
    "pug": "^3.0.2"
  },
  "devDependencies": {
    "babel-loader": "^9.1.2",
    "concurrently": "^8.0.1",
    "connect-livereload": "^0.6.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "livereload": "^0.9.3",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "webpack": "^5.81.0",
    "webpack-cli": "^5.0.2"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "frontend/**/*.js": "webpack"
  }
}
